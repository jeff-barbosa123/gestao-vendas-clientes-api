{
  "name": "gestao-vendas-clientes-api",
  "version": "1.0.0",
  "description": "API Rest para gestão de vendas e clientes com autenticação JWT e Swagger",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon ./src/server.js",
    "test": "mocha --config .mocharc.json",
    "test:report": "mocha --config .mocharc.json --reporter mochawesome --reporter-options reportDir=reports,reportFilename=index,quiet=true,overwrite=true,html=true,json=false",
    "test:watch": "mocha --config .mocharc.json --watch"
  },
  "keywords": [
    "express",
    "jwt",
    "swagger",
    "vendas",
    "clientes"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "pdfkit": "^0.15.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "chai": "^5.1.1",
    "dotenv": "^16.4.5",
    "mocha": "^10.7.0",
    "mochawesome": "^7.1.3",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0"
  }
}
